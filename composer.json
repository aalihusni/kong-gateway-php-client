{
    "name": "nasrulhazim/kong-gateway-php-client",
    "description": "Kong Gateway Admin API PHP Client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim Bin Mohamad",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "7.4.x-dev",
        "illuminate/support": "9.x-dev",
        "vlucas/phpdotenv": "5.3.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "KongGateway\\": "src/"
        },
        "files": [
            "support/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "bin": ["gate"]
}
